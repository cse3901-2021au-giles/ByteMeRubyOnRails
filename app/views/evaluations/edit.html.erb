<h1>Editing Evaluation</h1>

<%= form_with(model: @evaluation, local: true) do |f| %>
    <%if @evaluation%>
    <%= render 'shared/error_messages', :errors => @evaluation.errors %>
    <%end%>
    <%= f.label :score %>
    <%= f.text_field :score, class: 'form-control' %>
  
    <%= f.label :assessment %>
    <%= f.text_field :assessment, class: 'form-control' %>
  
    <%= f.submit "Submit", class: "btn btn-primary" %>
  <% end %>

<%= link_to 'Back', evaluations_path %>
